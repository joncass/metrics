{
  parser: "babel-eslint",
  "rules": {
    "brace-style": [2, "stroustrup"],
    "capitalized-comments": [2, "always"],
    "line-comment-position": [2, "above"],
    "max-len": [2, 80, 2],
    "require-jsdoc": [2],
    "valid-jsdoc": [2],
  },
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true,
  },
  "globals": {
    "apisReady": true,
    "window": true,
  },
}